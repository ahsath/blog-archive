{
  "name": "blog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon -x ts-node server.ts",
    "build": "pnpm build:client && pnpm build:server && pnpm prod",
    "prod": "NODE_OPTIONS=\"--loader ts-node/esm\" NODE_ENV=production node server.ts",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.ts"
  },
  "dependencies": {
    "@fastify/middie": "^8.1.0",
    "@fastify/static": "^6.6.0",
    "@fastify/view": "^7.3.0",
    "fastify": "^4.10.2",
    "fastify-plugin": "^4.4.0",
    "jsdom": "^20.0.3",
    "liquidjs": "^10.3.3",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.17",
    "@vitejs/plugin-vue": "^4.0.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.11"
  }
}
